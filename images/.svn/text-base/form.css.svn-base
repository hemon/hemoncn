.prb {
	text-align:left;
}
.prb h1 span a{
	padding-right:10px;
	float:right;
	background: url(../images/edit_personal.gif) no-repeat left;
	padding-left: 20px;
	color: #447cc1;
	font: bold 13px/20px "宋体";
	height:20px;
	text-decoration:none;
}
.prb h1 span a:hover{
	text-decoration:underline;
}
.prb h2{
	font: bold 14px Verdana, Arial, Helvetica, sans-serif, "宋体";
	color: #999999;
	height:22px;
	line-height:22px;
	border-bottom: 1px dotted #cccccc;
	clear:left;
	padding:0px 0px 0px 10px;
	margin:5px;
}

/*form元素定位*/
 p{
	color:#333333;
	text-align:left;
	clear: left;
	padding:1px;
	margin:5px;
	font:12px/20px Arial, Helvetica, sans-serif, "宋体";
}
form p label{
	width:90px;
	color:#333333;
	text-align:right;
	float:left;
	margin-right:20px;
	line-height:27px;
}
p span{
	width:150px;
	color:#666666;
	padding:0px 0px 3px 20px;
	margin:0px 0px 0px 10px;
	white-space:nowrap;
}
p a{
	color:#FF6600;
	text-decoration:underline;
}
p label{
	width:90px;
	color:#333333;
	text-align:right;
	float:left;
	margin-right:10px;
}
p label img{
border:1px solid #f1f1f1;
margin:1px;
}
form{
	margin:10px 0px 0px 0px;
}
form input,textarea,select{
	font:12px Arial, Helvetica, sans-serif, "宋体";
	padding:2px 1px;
	margin:2px 0px;
}
form .border0{
	border:0px;
}