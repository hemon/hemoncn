/* clearing */
	.stretch,
	.clear {
		clear:both; 
		height:1px; 
		margin:0; 
		padding:0; 
		font-size: 15px;
		line-height: 1px;
	}
	.clearfix:after {
		content: "."; 
		display: block; 
		height: 0; 
		clear: both; 
		visibility: hidden;
	}
	* html>body .clearfix {
	}
	
	*+html .clearfix {
		/* Hides from IE7 \*/
		min-height: 1% ;
		/* End hide from Ie7 */
	}
	
/* end clearing */

