		<script language="javascript">			
			document.write("</div>");
			displayline();
			
			ResizeMinSize();
			
			//设定导航条宽度
			function ResizeMinSize()
			{				
				var	dlayerwidth	=	780;	

				if ( document.body.offsetWidth < dlayerwidth )
				{
					DetailNaviLayer.style.width	=	String((dlayerwidth - 32)) + "px";					
				}
				else
				{
					dlayerwidth	=	document.body.offsetWidth;
					DetailNaviLayer.style.width	=	String((dlayerwidth - 32)) + "px";
					return;					
				}
				
				ResizeDeNaviLayer(true);
			}							
			
			//隐藏指定ID的图层
			function SetRollBoxNone(divId)
			{
				var naviTable =	document.getElementById(divId);
				if (naviTable != null ) {
					naviTable.style.display = 'none';
				}
			}
			//设定进度条的最大隐藏时间
			window.setTimeout("SetRollBoxNone('InitCacheFrame')",9000);
		</script>
