<SCRIPT language="javascript">
function SetDataBaseJump(obj)
{
	if ( obj == null )
		return;
	var dbname	=	document.getElementById("nowdbname").innerHTML;
	var icount		=	obj.length;
	for (ix = 0;ix<icount;ix++)
	{
		if (String(dbname).toUpperCase() == String(obj[ix].text).toUpperCase() )
		{
			obj[ix].selected = true;
			break;
		}
		else
		{
			obj[ix].selected = false;
		}
	}		
}
try
{
	SetDataBaseJump(document.all.drpDataBaseJump);
}
catch (err)
{}
</SCRIPT>
