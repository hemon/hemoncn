		<script>
	function GoToDB(alink,host,code)
		{	
			var returnurl = host;
			var url = host;		
			var lid = getCookie("LID");
			if (lid)
			{				
				url = host + "kns50/loginid.aspx?uid=" + lid + "&return=" + escape(returnurl);
				alink.href = url;
			}
		}
